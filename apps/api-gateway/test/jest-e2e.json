{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../../..",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@libs/(.*)$": "<rootDir>/libs/$1/src",
    "^@apps/(.*)$": "<rootDir>/apps/$1/src"
  },
  "testTimeout": 30000
}
